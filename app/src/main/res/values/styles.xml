<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="divider_horizontal">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">1dp</item>
        <item name="android:background">?android:attr/listDivider</item>
    </style>

    <style name="divider_vertical">
        <item name="android:layout_width">1dp</item>
        <item name="android:layout_height">0dp</item>
        <item name="android:background">?android:attr/listDivider</item>
    </style>

    <style name="indicator_horizontal">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">5dp</item>
        <item name="android:background">@drawable/rectangle</item>
    </style>


    <style name="outlineButton" parent="Widget.MaterialComponents.Button.TextButton">
        <item name="android:paddingLeft">16dp</item>
        <item name="android:paddingRight">16dp</item>
        <item name="strokeColor">@color/congress_lue</item>
        <item name="strokeWidth">1dp</item>
    </style>

    <style name="ShapeAppearanceOverlay.App.CornerSize10Percent" parent="">
        <item name="cornerSize">10%</item>
    </style>


</resources>